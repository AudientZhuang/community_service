/* components/pannel-box-detail/pannel-box-detail.wxss */
.body-detail {
  position: relative;
  display: flex;
  padding: 30rpx;
  border-bottom: 1px solid #f5f5f5;
}
.detail-l-icon {
  width: 20px;
  height: 20px;
  margin-right: 11px;
}
.detail-l-icon image {
  width: 100%;
  height: 100%;
}
.detail-title {
  font-size: 16px;
}
.detail-r-icon{
  position: absolute;
  top: 28rpx;
  right: 50rpx;
  width: 50rpx;
  height: 50rpx;
}
.detail-r-icon image {
  width: 100%;
  height: 100%;
}