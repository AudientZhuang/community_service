.list-body {
  position: relative;
  display: flex;
  padding: 30rpx;
  align-items: center;
  border-bottom: 1px solid #f5f5f5;
  background-color: white;
  padding-top: 40rpx;
}

.list-item-icon {
  display: inline-block;
  width: 50rpx;
  margin-right: 20rpx;
}

.list-item-icon image {
  width: 100%;
  height: 50rpx;
  vertical-align: -2px;
}

.list-item-text {
  display: inline-block;
  font-size: 32rpx;
  color: #353535;
}